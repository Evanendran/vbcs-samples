{
    "pageModelVersion": "0.9.8",
    "title": "Page",
    "description": "VB page",
    "variables": {
        "partnerList": {
            "type": "vb/ServiceDataProvider",
            "defaultValue": {
                "endpoint": "Partner/getall_partners",
                "itemsPath": "items",
                "transforms": {
                    "request": {
                        "filter": "{{ $page.functions.filter }}"
                    }
                }
            }
        },
        "searchCriteria": {
            "type": {
                "text": "string",
                "type": "string"
            }
        },
        "partnerTypeOptions": {
            "type": "application:partnerTypeOptions[]"
        },
        "selectedAddress": {
            "type": "string",
            "defaultValue": "500 Oracle Parkway, Redwood Shores, CA"
        },
        "selectedCompanyNumber": {
            "type": "string"
        }
    },
    "chains": {
        "goToPartnerRegister": {
            "description": "",
            "variables": {},
            "root": "navigateToPage1",
            "actions": {
                "navigateToPage1": {
                    "module": "vb/action/builtin/navigateToPageAction",
                    "parameters": {
                        "history": "push",
                        "page": "partner-register"
                    }
                }
            }
        },
        "listSelectionChanged": {
            "description": "",
            "variables": {},
            "root": "assignVariables1",
            "actions": {
                "assignVariables1": {
                    "module": "vb/action/builtin/assignVariablesAction",
                    "parameters": {
                        "$page.variables.selectedCompanyNumber": {
                            "source": "{{ $event.detail.value[0] }}"
                        }
                    }
                }
            }
        },
        "filterRows": {
            "root": "assignVariables1",
            "actions": {
                "assignVariables1": {
                    "module": "vb/action/builtin/assignVariablesAction",
                    "parameters": {
                        "$page.variables.partnerList.filterCriteria": {
                            "source": [
                                {
                                    "name": "OrganizationName",
                                    "op": "co",
                                    "value": " {{ $page.variables.searchCriteria.text }}"
                                }
                            ]
                        }
                    }
                }
            },
            "description": ""
        },
        "goToPartnerDetail": {
            "description": "",
            "variables": {},
            "root": "navigateToPage1",
            "actions": {
                "navigateToPage1": {
                    "module": "vb/action/builtin/navigateToPageAction",
                    "parameters": {
                        "history": "push",
                        "page": "partner-detail"
                    }
                }
            }
        }
    },
    "eventListeners": {
        "ojButton17816068091Click": {
            "chains": [
                {
                    "chainId": "goToPartnerRegister"
                }
            ]
        },
        "ojListView17816068091Selection": {
            "chains": [
                {
                    "chainId": "listSelectionChanged"
                }
            ]
        },
        "ojButton17816068092Click": {
            "chains": [
                {
                    "chainId": "filterRows"
                }
            ]
        },
        "a17816068091Click": {
            "chains": [
                {
                    "chainId": "goToPartnerDetail"
                }
            ]
        }
    },
    "imports": {
        "components": {
            "ojx-google-map": {
                "path": "resources/components/ojx-google-map/loader"
            }
        }
    }
}