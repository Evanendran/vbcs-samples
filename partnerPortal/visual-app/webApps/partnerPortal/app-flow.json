{
  "applicationModelVersion": "0.9.7",
  "id": "starter",
  "description": "A new VB app",
  "shellPage": "shell",
  "defaultPage": "partnerFinder",
  "pages": [ "partnerFinder", "partnerDetails", "partnerRegistration", "partnerProgram" ],
  "chains": {},
  "services": {
        "partners2": "../../resources/services/partners.json"
  },
    "variables": {
        "googleApiKey" : {
            "type": "string",
            "defaultValue" : "AIzaSyBgh8dJ-9yUUORwQBD1YEXeCd4PBxplhqg"
        },
        "appName": {
          "type": "string",
          "defaultValue": "Generic Partner Portal",
          "description": "The title of the application",
          "required": false
        },
        "userInfo": {
          "type": "object",
          "definition": "*",
          "defaultValue": {
            "email": "{{ $application.variables.userLogin }}",
            "initials": "{{ $application.variables.userInitials }}",
            "fullname": "{{ $application.variables.userFullname }}"
          }
        },
        "userLogin": {
          "type": "string",
          "defaultValue": "john.hancock@oracle.com"
        },
        "userInitials": {
          "type": "string",
          "defaultValue": "JH"
        },
        "userFullname": {
          "type": "string",
          "defaultValue": "John Hancock"
        }
    },
    "types": {
        "contact": {
          "type": "object",
          "definition": {
            "FirstName": {
              "type": "string",
              "description": "The first name"
            },
            "LastName": {
              "type": "string",
              "description": "The last name"
            },
            "WorkPhoneNumber": {
              "type": "string",
              "description": "phone"
            },
            "PrimaryFlag": {
              "type": "boolean",
              "description": "is this primary?"
            },
            "EmailAddress": {
              "type": "string",
              "description": "The email"
            },
            "Country": {
              "type": "string"
            }
          }
        },
        "partner": {
            "definition": {
                "AddressLineOne": {
                    "type": "string"
                },
                "AddressLineThree": {
                    "type": "string"
                },
                "AddressLineTwo": {
                    "type": "string"
                },
                "City": {
                    "type": "string"
                },
                "CompanyNumber": {
                    "type": "string"
                },
                "Country": {
                    "type": "string"
                },
                "EmailAddress": {
                    "type": "string"
                },
                "EmployeesTotal": {
                    "type": "number"
                },
                "OrganizationName": {
                    "type": "string"
                },
                "ParentPartnerId": {
                    "type": "string"
                },
                "ParentPartnerName": {
                    "type": "string"
                },
                "PartyId": {
                    "type": "string"
                },
                "PostalCode": {
                    "type": "string"
                },
                "PrimaryPhoneNumber": {
                    "type": "string"
                },
                "State": {
                    "type": "string"
                },
                "TierName": {
                    "type": "string"
                },
                "Url": {
                    "type": "string"
                },
                "YearEstablished": {
                    "type": "number"
                },
                "contacts": {
                    "definition": "application:contact",
                    "type": "array"
                },
                "industries": {
                    "definition": {
                        "ClassificationName": {
                            "description": "Industry Classification Name",
                            "type": "string"
                        }
                    },
                    "description": "Array of Partner Types",
                    "type": "array"
                },
                "types": {
                    "definition": {
                        "PartnerTypeName": {
                            "description": "Partner Type Name",
                            "type": "string"
                        }
                    },
                    "description": "Array of Partner Types",
                    "type": "array"
                }
            },
            "type": "object"
        }
    }
}
